    <DataSet Name="vuan">
      <Query>
        <DataSourceName>vksbd</DataSourceName>
        <CommandText>/****** Script for SelectTopNRows command from SSMS  ******/
SELECT va.*,  h.Ten as TenHuyen
  from Huyen h left join VuAn va on h.Id = va.HuyenId
  left join TinhTrangVuAn tr on tr.Id =  va.TinhTrangId	

  where tr.Code ='trahosodieutrabosung'</CommandText>
